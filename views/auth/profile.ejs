<h2 class="hello">Welcome <%= user.username %></h2>
<div class="profileButtons">
<button class="new-note" type="submit"><a href="/note/create">New Note...</a></button>

<% allNotes.forEach(note => { %>
    <h1><%=note.title%></h1>
    
    <button class="note-details" ><a href="/note/<%=note._id %>">Note Details...</a></button>
    <form mothod="POST" action="/note/<%=note._id %>/delete">
        <button class="delete-button" type="submit"><a class="deleteButton" href="/note/<%=note._id %>/delete">Delete...</a></button>
    </form>
    <% }) %> </div>

